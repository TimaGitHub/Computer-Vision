const isReadInput=document.getElementById("article__done-input");null==isReadInput||isReadInput.addEventListener("change",(async()=>{const e=isReadInput.dataset.article,t=isReadInput.checked;try{const a=`${e}/read`;isReadInput.setAttribute("disabled",!0);const d=await fetch(a,{method:"PUT",headers:{"Content-Type":"application/json;charset=utf-8"},body:JSON.stringify({isRead:t})});if(d.ok){let e=await d.json();isReadInput.checked=e.isRead}else isReadInput.checked=!t}catch(e){isReadInput.checked=!t}finally{isReadInput.removeAttribute("disabled")}}));